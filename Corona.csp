	.EQU	@, $000	
	.EQU	KEYBOARD_STATUS, $000
	.EQU	KEYBOARD_DATA, $001
	.EQU	MEMORY, 50
PASSWORD:	.CHAR, 'get pwned'
PASSCOMP:	.EQU, ''

	LDX#	0
	LDA#	0
KEYPOLL:	INB	KEYBOARD_STATUS
	NOP
	JGE	KEYPOLL
	INB	KEYBOARD_DATA
	STA+	$D00
	ADX#	1
	JMP	KEYPOLL

WORDCHECK:	PSHX
	LDX#	0
	
	LDC	$D00


DELETE_START:	LDX#	0
	LDA	0
DELETE_LOOP:	LDA+	0
	STA+	0
	AOC	MEMORY
	JGE	END
	JMP	DELETE_LOOP

END:	HLT
